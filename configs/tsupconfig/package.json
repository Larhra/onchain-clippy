{
  "name": "@onchain-clippy/tsupconfig",
  "version": "1.0.0",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "exports": {
		".": {
			"import": {
				"types": "./types/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
  "main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "types/index.d.ts",
  "scripts": {
    "all": "bun run build && bun run test:run && bun run generate:docs",
    "build": "nx build:dist && nx build:types",
    "build:dist": "tsup",
    "build:types": "tsup --dts-only && tsc --emitDeclarationOnly --declaration",
    "generate:docs": "typedoc --plugin typedoc-plugin-markdown",
    "clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
    "test:run": "tsup --dts-only"
  },
	"files": [
		"src",
		"dist",
		"types"
	],
  "dependencies": {
    "@onchain-clippy/tsconfig": "workspace:^",
    "@types/node": "^22.10.2"
  }
}
